{
  "name": "cloudbuild",
  "private": true,
  "version": "0.1.0",
  "description": "cloud build script for Ecom-strapi-stripe",
  "scripts": {
    "server": "cd server && npm install && npm run build",
    "client": "cd client && npm install && npm run build",
    "start:client": "cd client && npm run start",
    "start:server": "cd server && npm run start",
    "start": "npm run start:server & npm run start:client"
  },
  "devDependencies": {
    "dotenv": "^8.2.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "node-sass": "^4.13.1",
    "react-apollo": "^3.1.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.1.0"
  }
}
